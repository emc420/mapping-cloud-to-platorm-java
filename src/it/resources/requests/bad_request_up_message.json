        "humidity": {
          "value": "{{packet.message.humidity}}",
          "eventTime": "{{time}}",
          "type": "double",
          "unitId": "%RH"
        },
        "light": {
          "value": "{{packet.message.light}}",
          "eventTime": "{{time}}",
          "type": "double",
          "unitId": "lx"
        }
      }
    }
  ]
}
